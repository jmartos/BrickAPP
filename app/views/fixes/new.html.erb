<%= form_for(@fix, url: fix_create_path) do |f| %>
<%= render "error" %>
<div class="col-md-12"><%= link_to "Back", :back %><br><br></div>
<div id="edit" class="col-md-12">
  <div class="col-md-4"></div>
  <div class="col-md-8">
    <h3>Add new fix</h3> 
    <%= render "form", f: f %>
  </div>
</div>
<div class="col-md-5"></div>
<div class="col-md-1">
  <div class="actions">
    <%= f.submit "Create", class: "btn btn-primary" %><br>
  </div>
</div>
<% end %>
