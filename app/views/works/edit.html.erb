<%= form_for @work do |f| %>
<%= render "error" %>
<div class="col-md-12"><%= link_to 'Back', profile_path(tab: :works) %></div>
<div id="edit" class="col-md-12">
  <div class="col-md-2"></div>
  <div class="col-md-10">
    <h3>Edit work</h3> 
    <%= render "form", f: f %>
  </div>
</div>
<div class="col-md-5"></div>
<div class="col-md-1">
  <div class="actions">
    <%= f.submit "Update", class: "btn btn-primary" %><br>
  </div>
</div>
<% end %>
